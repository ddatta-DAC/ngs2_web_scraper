    <div id="tagline-block"><div class="container">
      <div class="row">
        <h1 id="tagline" class="col-xs-12">Research data management simplified.</h1>
        <div id="tag-container">
          <a class="tag" href="#transfer"><img src="/sites/all/themes/globus_bootstrap_theme/home_images/i_transfer_tag.svg"/>TRANSFER</a>
          <a class="tag" href="#sharing"><img src="/sites/all/themes/globus_bootstrap_theme/home_images/i_share_tag.svg"/>SHARE</a>
          <a class="tag" href="#publish"><img src="/sites/all/themes/globus_bootstrap_theme/home_images/i_publish_tag.svg"/>PUBLISH</a>
          <a class="tag" href="#build"><img src="/sites/all/themes/globus_bootstrap_theme/home_images/i_build_tag.svg"/>BUILD</a>
        </div>
      </div>
    </div></div>
